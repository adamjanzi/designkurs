<!DOCTYPE html>
<html class="no-js" lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link rel="icon" href="{{ theme_url }}/../shared/img/favicon.ico">
    <title>{% if meta.title %}{{ meta.title }} | {% endif %}{{ site_title }}</title>

    {% if meta.description %}
        <meta name="description" content="{{ meta.description|striptags }}" />
    {% endif %}
    {% if meta.robots %}
        <meta name="robots" content="{{ meta.robots }}" />
    {% endif %}

    {% if current_page %}
        <link rel="canonical" href="{{ current_page.url }}" />
    {% endif %}

    {% if config.session.theme == "dark" %}
        <link rel="stylesheet" href="{{ theme_url }}/css/style-dark.css" type="text/css" />
    {% else %}
        <link rel="stylesheet" href="{{ theme_url }}/css/style.css" type="text/css" />
    {% endif %}

</head>
<body{% if config.theme_config.widescreen %} class="widescreen"{% endif %}>
    <header class="site-header">
        <a class="nav-toggle" title="Toggle Menu" role="button" aria-controls="nav" aria-expanded="false" tabindex="1">
            <i class="fa fa-bars" aria-hidden="true" id="toggler"></i>
            <span class="sr-only">Toggle Menu</span>
        </a>
        <div class="nav" role="navigation" tabindex="-1">
            <div class="headersocial">
                {% for headersocial in pages["_meta"].meta.headersocial %}
                    <a href="{{ headersocial.url }}" title="{{ headersocial.title }}" role="button">
                        <i class="{{ headersocial.icon }}" aria-hidden="true"></i>
                        <span class="sr-only">{{ headersocial.title }}</span>
                    </a>
                {% endfor %}
            </div>
            <h1>Min design-sida</h1>
            <ul>
                {% for page in pages(depthOffset=-1) if page.title and not page.hidden %}
                <li{% if page.id == current_page.id %} class="active" {% endif %}>
                    <a href="{{ page.url }}">{{ page.title }}</a>
                    </li>
                    {% endfor %}
            </ul>
        </div>
        <div class="flash-region">
            <img class="flash-img" alt="Concert Photo" src="{{ base_url }}/image/newflash2.jpg?sc=banner1&a=40,0,0,0">
        </div>
    </header>
    <div class="main" id="main" role="main">
        <div class="gallery-container-grid">
            <div class="sidetitle">
                {{ content }}
            </div>
            <div class="kmom-box picture-box1">
                <a href="gallery/picture1" target="_blank">
                    <picture>
                        <source media="(min-width: 961px)" alt="Stage Photo" srcset="{{ base_url }}/image/dtlive.jpg?w=500&h=250&crop-to-fit&no-upscale&q=50">
                        <source media="(min-width: 376px)" alt="Stage Photo" srcset="{{ base_url }}/image/dtlive.jpg?w=450&h=225&crop-to-fit&no-upscale&q=50">
                        <img class="gallery-img" alt="Stage Photo" src="{{ base_url }}/image/dtlive.jpg?w=300&h=150&crop-to-fit&no-upscale&q=50">
                    </picture>
                </a>
            </div>
            <div class="kmom-box picture-box2">
                <a href="gallery/picture2" target="_blank">
                    <picture>
                        <source media="(min-width: 961px)" alt="Photo Of Me Drumming" srcset="{{ base_url }}/image/medrums.jpg?w=500&h=250&crop-to-fit&no-upscale&q=50">
                        <source media="(min-width: 376px)" alt="Photo Of Me Drumming" srcset="{{ base_url }}/image/medrums.jpg?w=450&h=225&crop-to-fit&no-upscale&q=50">
                        <img class="gallery-img" alt="Photo Of Me Drumming" src="{{ base_url }}/image/medrums.jpg?w=300&h=150&crop-to-fit&no-upscale&q=50">
                    </picture>
                </a>
            </div>
            <div class="kmom-box picture-box3">
                <a href="gallery/picture3" target="_blank">
                    <picture>
                        <source media="(min-width: 961px)" alt="Photo Of Me Drumming" srcset="{{ base_url }}/image/tour1.jpg?w=500&h=250&crop-to-fit&no-upscale&q=50">
                        <source media="(min-width: 376px)" alt="Photo Of Me Drumming" srcset="{{ base_url }}/image/tour1.jpg?w=450&h=225&crop-to-fit&no-upscale&q=50">
                        <img class="gallery-img" alt="Photo Of Me Drumming" src="{{ base_url }}/image/tour1.jpg?w=300&h=150&crop-to-fit&no-upscale&q=50">
                    </picture>
                </a>
            </div>
            <div class="kmom-box picture-box4">
                <a href="gallery/picture4" target="_blank">
                    <picture>
                        <source media="(min-width: 961px)" alt="Photo Sidestage Before A Show" srcset="{{ base_url }}/image/tourprep.jpg?w=500&h=250&crop-to-fit&no-upscale&q=50">
                        <source media="(min-width: 376px)" alt="Photo Sidestage Before A Show"  srcset="{{ base_url }}/image/tourprep.jpg?w=450&h=225&crop-to-fit&no-upscale&q=50">
                        <img class="gallery-img" alt="Photo Sidestage Before A Show" src="{{ base_url }}/image/tourprep.jpg?w=300&h=150&crop-to-fit&no-upscale&q=50">
                    </picture>
                </a>
            </div>
            <div class="kmom-box picture-box5">
                <a href="gallery/picture5" target="_blank">
                    <picture>
                        <source media="(min-width: 961px)" alt="Photo Of My Drumkit In The Studio" srcset="{{ base_url }}/image/topfloor.jpg?w=500&h=250&crop-to-fit&no-upscale&q=50">
                        <source media="(min-width: 376px)" alt="Photo Of My Drumkit In The Studio" srcset="{{ base_url }}/image/topfloor.jpg?w=450&h=225&crop-to-fit&no-upscale&q=50">
                        <img class="gallery-img" alt="Photo Of My Drumkit In The Studio" src="{{ base_url }}/image/topfloor.jpg?w=300&h=150&crop-to-fit&no-upscale&q=50">
                    </picture>
                </a>
            </div>
            <div class="kmom-box picture-box6">
                <a href="gallery/picture6" target="_blank">
                    <picture>
                        <source media="(min-width: 961px)" alt="Photo Of A Liveshow From The Crowd" srcset="{{ base_url }}/image/livepic.jpg?w=500&h=250&crop-to-fit&no-upscale&q=50">
                        <source media="(min-width: 376px)" alt="Photo Of A Liveshow From The Crowd" srcset="{{ base_url }}/image/livepic.jpg?w=450&h=225&crop-to-fit&no-upscale&q=50">
                        <img class="gallery-img" alt="Photo Of A Liveshow From The Crowd" src="{{ base_url }}/image/livepic.jpg?w=300&h=150&crop-to-fit&no-upscale&q=50">
                    </picture>
                </a>
            </div>
            </div>
    </div>
     <div class="footer">
        <div class="container">
            <div class="social">
                {% for social in pages["_meta"].meta.social %}
                    <a href="{{ social.url }}" title="{{ social.title }}" role="button">
                        <i class="{{ social.icon }}" aria-hidden="true"></i>
                        <span class="sr-only">{{ social.title }}</span>
                    </a>
                {% endfor %}
            </div>
        </div>
    </div>
    <script src="{{ theme_url }}/../shared/js/modernizr-3.3.1-custom.min.js" type="text/javascript"></script>
    <script src="{{ theme_url }}/../shared/js/utils.js" type="text/javascript"></script>
    <script src="{{ theme_url }}/../shared/js/responsive-navbar.js" type="text/javascript"></script>
</body>
